{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "build/client",
  "framework": "vite",
  "installCommand": "npm install",
  "routes": [
    {
      "src": "/health",
      "dest": "/api/health"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}